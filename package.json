{
	"name": "ghloc-web",
	"private": true,
	"scripts": {
		"dev": "pnpm build:languages && next dev",
		"build": "pnpm build:languages && pnpm build:sitemap && pnpm build:app",
		"build:languages": "tsx scripts/languages.ts",
		"build:sitemap": "tsx scripts/sitemap.ts",
		"build:app": "next build",
		"start": "next start",
		"prepare": "husky install",
		"lint": "next lint",
		"tsc": "tsc --noEmit",
		"format": "prettier './**/*.{js,jsx,ts,tsx,css,json,md,yml}' --write",
		"format:check": "prettier --check './**/*.{js,jsx,ts,tsx,css,json,md,yml}'",
		"ci-check": "npm-run-all format:check --parallel lint tsc",
		"postinstall": "is-ci || yarn prepare"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.15",
		"@heroicons/react": "^1.0.6",
		"@tanstack/react-query": "^4.29.19",
		"@vercel/analytics": "^1.0.1",
		"@vercel/og": "^0.5.8",
		"classnames": "^2.3.2",
		"dayjs": "^1.11.9",
		"next": "^13.4.9",
		"nprogress": "^0.2.0",
		"ofetch": "^1.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hot-toast": "^2.4.1",
		"tiny-server-timing": "^0.1.4",
		"zustand": "^4.3.9"
	},
	"devDependencies": {
		"@types/js-cookie": "^3.0.3",
		"@types/node": "^18.16.19",
		"@types/nprogress": "^0.2.0",
		"@types/react": "^18.2.14",
		"autoprefixer": "^10.4.14",
		"csstype": "^3.1.2",
		"eslint": "^8.44.0",
		"eslint-config-next": "^13.4.9",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^13.2.3",
		"next-statoscope": "^1.0.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.25",
		"prettier": "^3.0.0",
		"tailwindcss": "^3.3.2",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"webpack": "^5.88.1"
	}
}
